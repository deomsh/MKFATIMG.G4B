:lsspaces
#-#+ Needed for spaced LFN's
setlocal
set mdbase=0x300
set "ARG=%~1"
pause --wait=0 "%~2" > (md)%mdbase%+1
set numspace= && set skip=1
raw cat --locate=\x20 (md)%mdbase%+1 > nul ;; set /a numspace=%@retval% > nul
#NOTNOW#if %numspace%==0 && raw cat (md)%mdbase%+1 | set %ARG%= &; call set "%ARG%=%%%^ARG:~1,-1%%%" &; endlocal && call set "%ARG%=%%%ARG%%%" && goto :eof
if %numspace%==0 && if "%~1"=="PATH" && raw cat (md)%mdbase%+1 | set PATH= &; set "PATH=%PATH:~1,-1%" &; endlocal && set "PATH=%PATH%" && goto :eof
if %numspace%==0 && if "%~1"=="ARGNAME" && raw cat (md)%mdbase%+1 | set ARGNAME= &; set "ARGNAME=%ARGNAME:~1,-1%" &; endlocal && set "ARGNAME=%ARGNAME%" && goto :eof
if %numspace%==0 && if "%~1"=="ARGEXT" && raw cat (md)%mdbase%+1 | set ARGEXT= &; set "ARGEXT=%ARGEXT:~1,-1%" &; endlocal && set "ARGEXT=%ARGEXT%" && goto :eof
:looplsspaces
raw cat --skip=%skip% --locate=\x20 --number=1 (md)%mdbase%+1 > nul &; set /a skip=%?% > nul
set /A seek=%skip%+1 > nul
raw dd if=(md)%mdbase%+1 of=(md)%mdbase%+1 bs=1 skip=%skip% seek=%seek% > nul
raw cat --skip=%skip% --locate=\x20 --replace=\x5C --number=1 (md)%mdbase%+1 > nul
set /a numspace=%numspace%-1 > nul
#NOTNOW#if %numspace%==0 && raw cat (md)%mdbase%+1 | set %ARG%= &; call set "%ARG%=%%%^ARG:~1,-1%%%" &; endlocal && call set "%ARG%=%%%ARG%%%" && goto :eof
if %numspace%==0 && if "%~1"=="PATH" && raw cat (md)%mdbase%+1 | set PATH= &; set "PATH=%PATH:~1,-1%" &; endlocal && set "PATH=%PATH%" && goto :eof
if %numspace%==0 && if "%~1"=="ARGNAME" && raw cat (md)%mdbase%+1 | set ARGNAME= &; set "ARGNAME=%ARGNAME:~1,-1%" &; endlocal && set "ARGNAME=%ARGNAME%" && goto :eof
if %numspace%==0 && if "%~1"=="ARGEXT" && raw cat (md)%mdbase%+1 | set ARGEXT= &; set "ARGEXT=%ARGEXT:~1,-1%" &; endlocal && set "ARGEXT=%ARGEXT%" && goto :eof
set /A skip=%skip%+2 > nul
goto :looplsspaces
::
:LBAtoCHS
#-#+ Mapping sector number (LBA-address) to CHS; returns set-variable "result=set C=%C% && set H=%H% && set S=%S%"
#-# call :LBAtoCHS HEADS=heads SPT=spt LBA=lba (NO double quotes!; order NOT free; not case sensitive)
setlocal && set *
if not /i "%~1"=="HEADS" && if not /i "%~3"=="SPT" && endlocal && set result= && goto :eof
set /a HEADS=%~2 && set /a SPT=%~4 && shift && shift && shift && shift
if /i "%~1"=="LBA" && set /a LBA=%~2 ! endlocal && set result= && goto :eof
set /a HS=%HEADS% * %SPT% &; set /a C=%LBA%/%HS% &; set /a TMP=%LBA% % %HS% &; set /a H=%TMP%/%SPT% && set /a S=%TMP% % %SPT% + 1
endlocal && set "result=set C=%C% && set H=%H% && set S=%S%"
goto :eof
::
:CHStoLBA
#-#+ Mapping CHS to sector number (LBA-address); returns variable "result=LBA"
#-# call :CHStoLBA HEADS=heads SPT=spt C=C H=H S=S (NO double quotes!; order NOT free; not case sensitive)
setlocal && set *
if not /i "%~1"=="HEADS" && if not /i "%~3"=="SPT" && endlocal && set result= && goto :eof
set /a HEADS=%~2 && set /a SPT=%~4 && shift && shift && shift && shift
if not /i "%~1"=="C" && if not /i "%~3"=="H" && if not /i "%~5"=="S" && endlocal && set result= && goto :eof
set /a C=%~2 && set /a H=%~4 && set /a S=%~6 &; if %S%==0 && endlocal && set result= && goto :eof
set /a LBA=%C% * %HEADS% * %SPT% &; set /a LBA=%H% * %SPT% + %LBA% &; set /a LBA=%LBA%+%S%-1
endlocal && set result=%LBA%
goto :eof
